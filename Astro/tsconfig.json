{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@Components/*": ["src/components/*"],
      "@Hooks/*": ["src/hooks/*"],
      "@Apps/*": ["src/apps/*"],
      "@Services/*": ["src/services/*"],
      "@Utilities/*": ["src/utilities/*"],
      "@Layouts/*": ["src/layouts/*"],
      "@Assets/*": ["src/assets/*"],
      "@Context/*": ["src/context/*"],
      "@Pages/*": ["src/pages/*"]
    }
  }
}